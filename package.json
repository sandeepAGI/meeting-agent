{
  "name": "meeting-agent",
  "version": "0.1.0",
  "description": "AI-powered meeting transcription and summarization tool",
  "main": "dist/main/index.js",
  "author": "Sandeep Mangaraj",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "bash scripts/postinstall.sh",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron-vite preview",
    "test": "echo 'Test script will be configured with Jest'",
    "test:integration": "echo 'Integration test script will be configured'",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "package:mac": "npm run build && electron-builder --mac",
    "dist:mac": "npm run build && electron-builder --mac --publish never",
    "download-model": "bash scripts/download-whisper-model.sh",
    "db:migrate": "echo 'Database migration script will be added'",
    "db:reset": "echo 'Database reset script will be added'",
    "db:backup": "echo 'Database backup script will be added'"
  },
  "dependencies": {
    "electron": "^38.2.1",
    "electron-audio-loopback": "^1.0.6",
    "extendable-media-recorder": "^9.2.31",
    "extendable-media-recorder-wav-encoder": "^7.0.132",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.4",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
